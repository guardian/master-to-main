{
  "name": "@guardian/master-to-main",
  "type": "module",
  "description": "A CLI tool to rename master branch to main for GitHub repos",
  "version": "1.0.1",
  "author": "The Guardian",
  "bin": {
    "m2m": "./bin/run"
  },
  "bugs": "https://github.com/guardian/master-to-main/issues",
  "dependencies": {
    "@oclif/command": "^1.8.32",
    "@oclif/config": "^1.18.13",
    "@oclif/core": "^2.9.3",
    "@oclif/plugin-help": "^3",
    "@octokit/rest": "^19.0.7",
    "@octokit/types": "^6.11.1",
    "@types/prompts": "^2.4.0",
    "chalk": "^4.1.0",
    "log-symbols": "^4.0.0",
    "node-emoji": "^2.1.0",
    "ora": "^6.3.1",
    "prompts": "^2.4.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/jest": "^29.5.2",
    "@types/node": "^18",
    "eslint": "^7.17",
    "eslint-config-oclif": "^4.0",
    "eslint-config-oclif-typescript": "^0.2",
    "jest": "^29.6.1",
    "prettier": "^2.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10",
    "typescript": "^4.7"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/guardian/master-to-main",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "exports": "./index.js",
  "oclif": {
    "bin": "m2m"
  },
  "repository": "guardian/master-to-main",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --runInBand",
    "test:dev": "jest --detectOpenHandles --runInBand --watchAll",
    "lint": "eslint src --ext .ts",
    "version": "oclif-dev readme && git add README.md",
    "run:dev": "ts-node ./bin/dev.js"
  },
  "types": "lib/index.d.ts"
}
