{
  "name": "@guardian/master-to-main",
  "type": "module",
  "description": "A CLI tool to rename master branch to main for GitHub repos",
  "version": "2.0.0",
  "author": "The Guardian",
  "bin": {
    "m2m": "./bin/run"
  },
  "bugs": "https://github.com/guardian/master-to-main/issues",
  "dependencies": {
    "@oclif/command": "^1.8.32",
    "@oclif/config": "^1.18.13",
    "@oclif/core": "^2.9.3",
    "@oclif/plugin-help": "^5.2.13",
    "@octokit/rest": "^20.0.1",
    "@octokit/types": "^11.1.0",
    "@types/prompts": "^2.4.0",
    "chalk": "^5.3.0",
    "log-symbols": "^5.1.0",
    "node-emoji": "^2.1.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-oclif": "^4.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "jest": "^29.6.1",
    "oclif": "^3.9.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/guardian/master-to-main",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "exports": "./index.js",
  "oclif": {
    "bin": "m2m"
  },
  "repository": "guardian/master-to-main",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepack": "rm -rf lib && tsc -b && oclif readme",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --runInBand",
    "test:dev": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --runInBand --watchAll",
    "lint": "eslint src --ext .ts",
    "version": "oclif readme && git add README.md",
    "run:dev": "ts-node ./bin/dev.js"
  },
  "types": "lib/index.d.ts"
}
