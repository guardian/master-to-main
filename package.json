{
  "name": "@guardian/master-to-main",
  "description": "A CLI tool to rename master branch to main for GitHub repos",
  "version": "1.0.1",
  "author": "The Guardian",
  "bin": {
    "m2m": "./bin/run"
  },
  "bugs": "https://github.com/guardian/master-to-main/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@octokit/rest": "^18.3.0",
    "@octokit/types": "^6.11.1",
    "@types/node-emoji": "^1.8.1",
    "@types/prompts": "^2.4.0",
    "chalk": "^4.1.0",
    "log-symbols": "^4.0.0",
    "node-emoji": "^1.10.0",
    "ora": "^5.0.0",
    "prompts": "^2.4.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/jest": "^26.0.13",
    "@types/node": "^15",
    "eslint": "^7.17",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^10",
    "typescript": "^4.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/guardian/master-to-main",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "bin": "m2m"
  },
  "repository": "guardian/master-to-main",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "jest --detectOpenHandles --runInBand",
    "test:dev": "jest --detectOpenHandles --runInBand --watchAll",
    "lint": "eslint src --ext .ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
